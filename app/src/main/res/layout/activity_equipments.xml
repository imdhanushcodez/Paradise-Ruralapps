<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".equipments"
    android:orientation="vertical"
    android:background="#D6E3A8">
    <LinearLayout
        android:layout_width="407dp"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:elevation="20dp"
        android:background="@color/white">
        <androidx.appcompat.widget.AppCompatImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/la"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="30dp"
            android:id="@+id/existep">
        </androidx.appcompat.widget.AppCompatImageButton>
        <TextView
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:text="EQUIMENTS"
            android:textColor="#73980b"
            android:textStyle="bold"
            android:fontFamily="@font/bruno"
            android:textSize="17dp"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="60dp">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="407dp"
        android:layout_height="730dp">
        <ScrollView
            android:layout_width="407dp"
            android:layout_height="730dp">
            <LinearLayout
                android:layout_width="407dp"
                android:layout_height="730dp"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/eq1"
                    android:elevation="10dp">
                    <androidx.appcompat.widget.AppCompatButton
                        android:layout_width="90dp"
                        android:layout_height="30dp"
                        android:text="book"
                        android:textColor="@color/white"
                        android:background="@drawable/agri11"
                        android:layout_marginLeft="190dp"
                        android:layout_marginTop="210dp"
                        android:id="@+id/eqbtn1">
                    </androidx.appcompat.widget.AppCompatButton>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/eq2">
                    <androidx.appcompat.widget.AppCompatButton
                        android:layout_width="90dp"
                        android:layout_height="30dp"
                        android:text="book"
                        android:textColor="@color/white"
                        android:background="@drawable/agri11"
                        android:layout_marginLeft="190dp"
                        android:layout_marginTop="220dp"
                        android:elevation="10dp"
                        android:id="@+id/eqbtn2">
                    </androidx.appcompat.widget.AppCompatButton>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/eq3">
                    <androidx.appcompat.widget.AppCompatButton
                        android:layout_width="90dp"
                        android:layout_height="30dp"
                        android:text="book"
                        android:textColor="@color/white"
                        android:background="@drawable/agri11"
                        android:layout_marginLeft="190dp"
                        android:layout_marginTop="220dp"
                        android:elevation="10dp"
                        android:id="@+id/eqbtn3">
                    </androidx.appcompat.widget.AppCompatButton>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/eq4">
                    <androidx.appcompat.widget.AppCompatButton
                        android:layout_width="90dp"
                        android:layout_height="30dp"
                        android:text="book"
                        android:textColor="@color/white"
                        android:background="@drawable/agri11"
                        android:layout_marginLeft="190dp"
                        android:layout_marginTop="220dp"
                        android:elevation="10dp"
                        android:id="@+id/eqbtn4">
                    </androidx.appcompat.widget.AppCompatButton>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/eq5">
                    <androidx.appcompat.widget.AppCompatButton
                        android:layout_width="90dp"
                        android:layout_height="30dp"
                        android:text="book"
                        android:textColor="@color/white"
                        android:background="@drawable/agri11"
                        android:layout_marginLeft="190dp"
                        android:layout_marginTop="220dp"
                        android:elevation="10dp"
                        android:id="@+id/eqbtn5">
                    </androidx.appcompat.widget.AppCompatButton>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="300dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="10dp">
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</LinearLayout>